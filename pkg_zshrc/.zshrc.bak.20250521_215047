alias 4tmux_h="tmux new-session -s 4tmux_h -d \; \
  split-window -h \; \
  split-window -h \; \
  split-window -h \; \
  attach-session -t 4tmux_h"

alias 4tmux_v="tmux new-session -s 4tmux_v -d \; \
  split-window -v \; \
  split-window -v \; \
  split-window -v \; \
  attach-session -t 4tmux_v"


alias 4tmux='tmux new-session -s 4tmux -d \; \
  split-window   -h           \; \
  split-window   -v -t 4tmux:0.0 \; \
  split-window   -v -t 4tmux:0.1 \; \
  attach-session -t 4tmux'

